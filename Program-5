#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
	int a=1234;
	float b=24.121234;
	cout<<"220130318102"<<endl;
	cout<<"My Name Is Krinshsmith Kava"<<endl;
	cout<<"The value of a is :: "<<setfill('@')<<setw(5)<<a<<endl;
	cout<<"The value of b is :: "<<setprecision(5)<<b<<endl;
	return 0;
	
}
